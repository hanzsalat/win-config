@{
    # Setting for what WM config should be used
    # home/work
    workspace = 'home'

    # Settings for what WM should be used
    # komorebi/galzewm
    windowmanager = 'glazewm'

    # Setting for the useage of Oh-My-Posh or Starthip
    # omp/starship
    prompt = 'omp'

    # Setting if the taskbar should be there or not
    # $true/$false
    taskbar = $false
}